#Numeric operators
a=12
b=3
#basic arithmatic operators
# and Expressons
print(a + b)    # 15
print(a - b)    # 3
print(a * b)    # 36
print(a / b)    # 4.0 floating point value
print(a // b)   # 4 integer division, rounded down towards minus infinity
print(a % b)    # 0 module: the remainder after intiger division

print()
for i in range(1,4):
    print(i)
    print()
#python is strongly typed
print()
for i in range(1,a//b):
    print(i)
#TypeError: 'float' object cannot be interpreted as an integer    
print()
for i in range(1,a/b):
    print(i)

i=1
print(i)
i=2
print(i)
i=3
print(i)
